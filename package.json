{
  "name": "scylla",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "figlet": "^1.2.0",
    "node-sass": "^4.5.0",
    "react-scripts": "1.0.7"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "npm-run-all": "^4.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-ga": "^2.1.2",
    "react-helmet": "^5.0.0",
    "react-localization": "0.0.12",
    "react-motion": "^0.4.7",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-dom": "^4.0.0-beta.6",
    "react-router-transitioner": "^0.1.4",
    "react-transition-group": "^1.2.0",
    "redux": "^3.6.0"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch",
    "start-js": "react-scripts start",
    "start": "node because && npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
